<?xml version="1.0" encoding="UTF-8"?>
<poem title="Любовь" author="Анна Андреевна Ахматова">
   <divs>
      <lg type="stanza">
         <l>
            <w>
               <orth>То</orth>
               <str>то
               </str>
            </w>
            <w>
               <orth>змейкой,</orth>
               <str>зм<stress>е</stress>йкой</str>
            </w>
            <w>
               <orth>свернувшись</orth>
               <str>сверн<stress>у</stress>вшись</str>
            </w>
            <w>
               <orth>клубком,</orth>
               <str>клубк<stress>о</stress>м</str>
            </w>
         </l>
         <l>
            <w>
               <orth>У</orth>
               <str>У</str>
            </w>
            <w>
               <orth>самого</orth>
               <str>с<stress>а</stress>мого
               </str>
            </w>
            <w>
               <orth>сердца</orth>
               <str>с<stress>е</stress>рдца</str>

            </w>
            <w>
               <orth>колдует,</orth>
               <str>колд<stress>у</stress>ет</str>
            </w>
         </l>
         <l>
            <w>
               <orth>То</orth>
               <str>то
               </str>
            </w>
            <w>
               <orth>целые</orth>
               <str>ц<stress>е</stress>лые</str>
            </w>
            <w>
               <orth>дни</orth>
               <str>дн<stress>и</stress>
               </str>
            </w>
            <w>
               <orth>голубком</orth>
               <str>голубк<stress>о</stress>м</str>
            </w>
         </l>
         <l>
            <w>
               <orth>На</orth>
               <str>На</str>
            </w>
            <w>
               <orth>белом</orth>
               <str>б<stress>е</stress>лом</str>
            </w>
            <w>
               <orth>окошке</orth>
               <str>ок<stress>о</stress>шке</str>
            </w>
            <w>
               <orth>воркует,</orth>
               <str>ворк<stress>у</stress>ет</str>
            </w>
         </l>
      </lg>
      <lg type="stanza">
         <l>
            <w>
               <orth>То</orth>
               <str>то
               </str>
            </w>
            <w>
               <orth>в</orth>
               <str>в</str>
            </w>
            <w>
               <orth>инее</orth>
               <str><stress>и</stress>нее</str>
            </w>
            <w>
               <orth>ярком</orth>
               <str>
                  <stress>я</stress>рком</str>

            </w>
            <w>
               <orth>блеснет,</orth>
               <str>блесн<stress>ё</stress>т</str>
            </w>
         </l>
         <l>
            <w>
               <orth>Почудится</orth>
               <str>поч<stress>у</stress>дится</str>
            </w>
            <w>
               <orth>в</orth>
               <str>в</str>
            </w>
            <w>
               <orth>дреме</orth>
               <str>др<stress>ё</stress>ме</str>
            </w>
            <w>
               <orth>левкоя...</orth>
               <str>левк<stress>о</stress>я</str>
            </w>
         </l>
         <l>
            <w>
               <orth>Но</orth>
               <str>Но</str>
            </w>
            <w>
               <orth>верно</orth>
               <str>в<stress>е</stress>рно</str>
            </w>
            <w>
               <orth>и</orth>
               <str>и</str>
            </w>
            <w>
               <orth>тайно</orth>
               <str>т<stress>а</stress>йно</str>
            </w>
            <w>
               <orth>ведет</orth>
               <str>вед<stress>ё</stress>т</str>
            </w>
         </l>
         <l>
            <w>
               <orth>От</orth>
               <str>От</str>
            </w>
            <w>
               <orth>радости</orth>
               <str>р<stress>а</stress>дости</str>
            </w>
            <w>
               <orth>и</orth>
               <str>и</str>
            </w>
            <w>
               <orth>от</orth>
               <str>от</str>
            </w>
            <w>
               <orth>покоя.</orth>
               <str>пок<stress>о</stress>я</str>
            </w>
         </l>
      </lg>
      <lg type="stanza">
         <l>
            <w>
               <orth>Умеет</orth>
               <str>ум<stress>е</stress>ет</str>
            </w>
            <w>
               <orth>так</orth>
               <str>так</str>
            </w>
            <w>
               <orth>сладко</orth>
               <str>сл<stress>а</stress>дко</str>
            </w>
            <w>
               <orth>рыдать</orth>
               <str>рыд<stress>а</stress>ть</str>
            </w>
         </l>
         <l>
            <w>
               <orth>В</orth>
               <str>В</str>
            </w>
            <w>
               <orth>молитве</orth>
               <str>мол<stress>и</stress>тве</str>
            </w>
            <w>
               <orth>тоскующей</orth>
               <str>тоск<stress>у</stress>ющей</str>
            </w>
            <w>
               <orth>скрипки,</orth>
               <str>скр<stress>и</stress>пки</str>
            </w>
         </l>
         <l>
            <w>
               <orth>И</orth>
               <str>И</str>
            </w>
            <w>
               <orth>страшно</orth>
               <str>стр<stress>а</stress>шно</str>
            </w>
            <w>
               <orth>ее</orth>
               <str>е<stress>ё</stress>
               </str>
            </w>
            <w>
               <orth>угадать</orth>
               <str>угад<stress>а</stress>ть</str>
            </w>
         </l>
         <l>
            <w>
               <orth>В</orth>
               <str>В</str>
            </w>
            <w>
               <orth>еще</orth>
               <str>еще</str>
            </w>
            <w>
               <orth>незнакомой</orth>
               <str>незнак<stress>о</stress>мой</str>
            </w>
            <w>
               <orth>улыбке.</orth>
               <str>ул<stress>ы</stress>бке</str>
            </w>
         </l>
      </lg>
   </divs>
   <date>1912</date><author>Анна Ахматова</author>
</poem>
