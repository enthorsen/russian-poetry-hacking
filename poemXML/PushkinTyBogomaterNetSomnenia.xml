<?xml version="1.0" encoding="UTF-8"?>
<poem author="Александр Сергеевич Пушкин" title="К ***" date="1826">
    <divs>
        <lg>
            <l>
                <w>
                    <orth>Ты</orth>
                    <str>(ты)</str>
                </w>
                <w>
                    <orth>богоматерь,</orth>
                    <str>богом<stress>а</stress>терь</str>
                </w>
                <w>
                    <orth>нет</orth>
                    <str>нет</str>
                </w>
                <w>
                    <orth>сомненья,</orth>
                    <str>сомн<stress>е</stress>нья</str>
                </w>
            </l>
            <l>
                <w>
                    <orth>Не</orth>
                    <str>не</str>
                </w>
                <w>
                    <orth>та,</orth>
                    <str>(та)</str>
                </w>
                <w>
                    <orth>которая</orth>
                    <str>кот<stress>о</stress>рая</str>
                </w>
                <w>
                    <orth>красой</orth>
                    <str>крас<stress>о</stress>й</str>
                </w>
            </l>
            <l>
                <w>
                    <orth>Пленила</orth>
                    <str>плен<stress>и</stress>ла</str>
                </w>
                <w>
                    <orth>только</orth>
                    <str>т<stress>о</stress>лько</str>
                </w>
                <w>
                    <orth>дух</orth>
                    <str>д<stress>у</stress>х</str>
                </w>
                <w>
                    <orth>святой,</orth>
                    <str>свят<stress>о</stress>й</str>
                </w>
            </l>
            <l>
                <w>
                    <orth>Мила</orth>
                    <str>мил<stress>а</stress></str>
                </w>
                <w>
                    <orth>ты</orth>
                    <str>(ты)</str>
                </w>
                <w>
                    <orth>всем</orth>
                    <str>(всем)</str>
                </w>
                <w>
                    <orth>без</orth>
                    <str>без</str>
                </w>
                <w>
                    <orth>исключенья;</orth>
                    <str>исключ<stress>е</stress>нья</str>
                </w>
            </l>
            <l>
                <w>
                    <orth>Не</orth>
                    <str>не</str>
                </w>
                <w>
                    <orth>та,</orth>
                    <str>(та)</str>
                </w>
                <w>
                    <orth>которая</orth>
                    <str>кот<stress>о</stress>рая</str>
                </w>
                <w>
                    <orth>Христа</orth>
                    <str>христ<stress>а</stress></str>
                </w>
            </l>
            <l>
                <w>
                    <orth>Родила</orth>
                    <str>род<stress>и</stress>ла</str>
                </w>
                <w>
                    <orth>не</orth>
                    <str>не</str>
                </w>
                <w>
                    <orth>спросясь</orth>
                    <str>спрос<stress>я</stress>сь</str>
                </w>
                <w>
                    <orth>супруга.</orth>
                    <str>супр<stress>у</stress>га</str>
                </w>
            </l>
            <l>
                <w>
                    <orth>Есть</orth>
                    <str>есть</str>
                </w>
                <w>
                    <orth>бог</orth>
                    <str>б<stress>о</stress>г</str>
                </w>
                <w>
                    <orth>другой</orth>
                    <str>друг<stress>о</stress>й</str>
                </w>
                <w>
                    <orth>земного</orth>
                    <str>земн<stress>о</stress>го</str>
                </w>
                <w>
                    <orth>круга—</orth>
                    <str>кр<stress>у</stress>га</str>
                </w>
            </l>
            <l>
                <w>
                    <orth>Ему</orth>
                    <str>ем<stress>у</stress></str>
                </w>
                <w>
                    <orth>послушна</orth>
                    <str>посл<stress>у</stress>шна</str>
                </w>
                <w>
                    <orth>красота,</orth>
                    <str>красот<stress>а</stress></str>
                </w>
            </l>
            <l>
                <w>
                    <orth>Он</orth>
                    <str>он</str>
                </w>
                <w>
                    <orth>бог</orth>
                    <str>б<stress>о</stress>г</str>
                </w>
                <w>
                    <orth>Парни,</orth>
                    <str>парн<stress>и</stress></str>
                </w>
                <w>
                    <orth>Тибулла,</orth>
                    <str>тиб<stress>у</stress>лла</str>
                </w>
                <w>
                    <orth>Мура,</orth>
                    <str>м<stress>у</stress>ра</str>
                </w>
            </l>
            <l>
                <w>
                    <orth>Им</orth>
                    <str>(им)</str>
                </w>
                <w>
                    <orth>мучусь,</orth>
                    <str>м<stress>у</stress>чусь</str>
                </w>
                <w>
                    <orth>им</orth>
                    <str>(им)</str>
                </w>
                <w>
                    <orth>утешен</orth>
                    <str>ут<stress>е</stress>шен</str>
                </w>
                <w>
                    <orth>я.</orth>
                    <str>я</str>
                </w>
            </l>
            <l>
                <w>
                    <orth>Он</orth>
                    <str>он</str>
                </w>
                <w>
                    <orth>весь</orth>
                    <str>(весь)</str>
                </w>
                <w>
                    <orth>в</orth>
                    <str>в</str>
                </w>
                <w>
                    <orth>тебя—</orth>
                    <str>теб<stress>я</stress></str>
                </w>
                <w>
                    <orth>ты</orth>
                    <str>(ты)</str>
                </w>
                <w>
                    <orth>мать</orth>
                    <str>м<stress>а</stress>ть</str>
                </w>
                <w>
                    <orth>Амура,</orth>
                    <str>ам<stress>у</stress>ра</str>
                </w>
            </l>
            <l>
                <w>
                    <orth>Ты</orth>
                    <str>(ты)</str>
                </w>
                <w>
                    <orth>богородица</orth>
                    <str>богор<stress>о</stress>дица</str>
                </w>
                <w>
                    <orth>моя!</orth>
                    <str>мо<stress>я</stress></str>
                </w>
            </l>
        </lg>
    </divs>
</poem>
