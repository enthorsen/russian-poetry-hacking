                    <orth>божества,</orth>
                    <str>божеств<stress>а</stress></str>
                </w>
                <w>
                    <orth>без</orth>
                    <str>без</str>
                </w>
                <w>
                    <orth>вдохновенья,</orth>
                    <str>вдохнов<stress>е</stress>нья</str>
                </w>
            </l>
            <l>
                <w>
                    <orth>Без</orth>
                    <str>без</str>
                </w>
                <w>
                    <orth>слёз,</orth>
                    <str>(слёз)</str>
                </w>
                <w>
                    <orth>без</orth>
                    <str>без</str>
                </w>
                <w>
                    <orth>жизни,</orth>
                    <str>ж<stress>и</stress>зни</str>
                </w>
                <w>
                    <orth>без</orth>
                    <str>без</str>
                </w>
                <w>
                    <orth>любви.</orth>
                    <str>любв<stress>и</stress></str>
                </w>
            </l>
        </lg>
        <lg type="stanza">
            <l>
                <w>
                    <orth>Душе</orth>
                    <str>душ<stress>е</stress></str>
                </w>
                <w>
                    <orth>настало</orth>
                    <str>наст<stress>а</stress>ло</str>
                </w>
                <w>
                    <orth>пробужденье:</orth>
                    <str>пробужд<stress>е</stress>нье</str>
                </w>
            </l>
            <l>
                <w>
                    <orth>И</orth>
                    <str>и</str>
                </w>
                <w>
                    <orth>вот</orth>
                    <str>вот</str>
                </w>
                <w>
                    <orth>опять</orth>
                    <str>оп<stress>я</stress>ть</str>
                </w>
                <w>
                    <orth>явилась</orth>
                    <str>яв<stress>и</stress>лась</str>
                </w>
                <w>
                    <orth>ты,</orth>
                    <str>(ты)</str>
                </w>
            </l>
            <l>
                <w>
                    <orth>Как</orth>
                    <str>как</str>
                </w>
                <w>
                    <orth>мимолётное</orth>
                    <str>мимол<stress>ё</stress>тное</str>
                </w>
                <w>
                    <orth>виденье,</orth>
                    <str>вид<stress>е</stress>нье</str>
                </w>
            </l>
            <l>
                <w>
                    <orth>Как</orth>
                    <str>как</str>
                </w>
                <w>
                    <orth>гений</orth>
                    <str>г<stress>е</stress>ний</str>
                </w>
                <w>
                    <orth>чистой</orth>
                    <str>ч<stress>и</stress>стой</str>
                </w>
                <w>
                    <orth>красоты.</orth>
                    <str>красот<stress>ы</stress></str>
                </w>
            </l>
        </lg>
        <lg type="stanza">
            <l>
                <w>
                    <orth>И</orth>
                    <str>и</str>
                </w>
                <w>
                    <orth>сердце</orth>
                    <str>с<stress>е</stress>рдце</str>
                </w>
                <w>
                    <orth>бьётся</orth>
                    <str>(бьётся)</str>
                </w>
                <w>
                    <orth>в</orth>
                    <str>в</str>
                </w>
                <w>
                    <orth>упоенье</orth>
                    <str>упо<stress>е</stress>нье</str>
                </w>
            </l>
            <l>
                <w>
                    <orth>И</orth>
                    <str>и</str>
                </w>
                <w>
                    <orth>для</orth>
                    <str>для</str>
                </w>
                <w>
                    <orth>него</orth>
                    <str>нег<stress>о</stress></str>
                </w>
                <w>
                    <orth>воскресли</orth>
                    <str>воскр<stress>е</stress>сли</str>
                </w>
                <w>
                    <orth>вновь</orth>
                    <str>вн<stress>о</stress>вь</str>
                </w>
            </l>
            <l>
                <w>
                    <orth>И</orth>
                    <str>и</str>
                </w>
                <w>
                    <orth>божество,</orth>
                    <str>божеств<stress>о</stress></str>
                </w>
                <w>
                    <orth>и</orth>
                    <str>и</str>
                </w>
                <w>
                    <orth>вдохновенье,</orth>
                    <str>вдохнов<stress>е</stress>нье</str>
                </w>
