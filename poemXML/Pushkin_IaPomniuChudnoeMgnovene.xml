            <l>
                <w>
                    <orth>И</orth>
                    <str>и</str>
                </w>
                <w>
                    <orth>жизнь,</orth>
                    <str>ж<stress>и</stress>знь</str>
                </w>
                <w>
                    <orth>и</orth>
                    <str>и</str>
                </w>
                <w>
                    <orth>слёзы,</orth>
                    <str>(слёзы)</str>
                </w>
                <w>
                    <orth>и</orth>
                    <str>и</str>
                </w>
                <w>
                    <orth>любовь.</orth>
                    <str>люб<stress>о</stress>вь</str>
                </w>
            </l>
        </lg>
