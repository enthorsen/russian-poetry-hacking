<?xml version="1.0" encoding="UTF-8"?>
<poem author="Александр Сергеевич Пушкин" title="Евгений Онегин" date="">
    <divs type="chapter">
        <lg>
            <l>
                <w>
                    <orth>"Мой</orth>
                    <str>(мой)</str>
                </w>
                <w>
                    <orth>дядя</orth>
                    <str>д<stress>я</stress>дя</str>
                </w>
                <w>
                    <orth>самых</orth>
                    <str>с<stress>а</stress>мых</str>
                </w>
                <w>
                    <orth>честных</orth>
                    <str>ч<stress>е</stress>стных</str>
                </w>
                <w>
                    <orth>правил,</orth>
                    <str>пр<stress>а</stress>вил</str>
                </w>
            </l>
            <l>
                <w>
                    <orth>Когда</orth>
                    <str>когд<stress>а</stress></str>
                </w>
                <w>
                    <orth>не</orth>
                    <str>не</str>
                </w>
                <w>
                    <orth>в</orth>
                    <str>в</str>
                </w>
                <w>
                    <orth>шутку</orth>
                    <str>ш<stress>у</stress>тку</str>
                </w>
                <w>
                    <orth>занемог,</orth>
                    <str>занем<stress>о</stress>г</str>
                </w>
            </l>
            <l>
                <w>
                    <orth>Он</orth>
                    <str>он</str>
                </w>
                <w>
                    <orth>уважать</orth>
                    <str>уваж<stress>а</stress>ть</str>
                </w>
                <w>
                    <orth>себя</orth>
                    <str>себ<stress>я</stress></str>
                </w>
                <w>
                    <orth>заставил</orth>
                    <str>заст<stress>а</stress>вил</str>
                </w>
            </l>
            <l>
                <w>
                    <orth>И</orth>
                    <str>и</str>
                </w>
                <w>
                    <orth>лучше</orth>
                    <str>л<stress>у</stress>чше</str>
                </w>
                <w>
                    <orth>выдумать</orth>
                    <str>в<stress>ы</stress>думать</str>
                </w>
                <w>
                    <orth>не</orth>
                    <str>не</str>
                </w>
                <w>
                    <orth>мог.</orth>
                    <str>(мог)</str>
                </w>
            </l>
            <l>
                <w>
                    <orth>Его</orth>
                    <str>(ево)</str>
                </w>
                <w>
                    <orth>пример</orth>
                    <str>прим<stress>е</stress>р</str>
                </w>
                <w>
                    <orth>другим</orth>
                    <str>друг<stress>и</stress>м</str>
                </w>
                <w>
                    <orth>наука;</orth>
                    <str>на<stress>у</stress>ка</str>
                </w>
            </l>
            <l>
                <w>
                    <orth>Но,</orth>
                    <str>но</str>
                </w>
                <w>
                    <orth>боже</orth>
                    <str>б<stress>о</stress>же</str>
                </w>
                <w>
                    <orth>мой,</orth>
                    <str>(мой)</str>
                </w>
                <w>
                    <orth>какая</orth>
                    <str>как<stress>а</stress>я</str>
                </w>
                <w>
                    <orth>скука</orth>
                    <str>ск<stress>у</stress>ка</str>
                </w>
            </l>
            <l>
                <w>
                    <orth>С</orth>
                    <str>с</str>
                </w>
                <w>
                    <orth>больным</orth>
                    <str>больн<stress>ы</stress>м</str>
                </w>
                <w>
                    <orth>сидеть</orth>
                    <str>сид<stress>е</stress>ть</str>
                </w>
                <w>
                    <orth>и</orth>
                    <str>и</str>
                </w>
                <w>
                    <orth>день</orth>
                    <str>д<stress>е</stress>нь</str>
                </w>
                <w>
                    <orth>и</orth>
                    <str>и</str>
                </w>
                <w>
                    <orth>ночь,</orth>
                    <str>н<stress>о</stress>чь</str>
                </w>
            </l>
            <l>
                <w>
                    <orth>Не</orth>
                    <str>не</str>
                </w>
                <w>
                    <orth>отходя</orth>
                    <str>отход<stress>я</stress></str>
                </w>
                <w>
                    <orth>ни</orth>
                    <str>ни</str>
                </w>
                <w>
                    <orth>шагу</orth>
                    <str>ш<stress>а</stress>гу</str>
                </w>
