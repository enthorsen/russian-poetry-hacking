<?xml version="1.0" encoding="UTF-8"?>
<poem title="Лилии" author="Анна Андреевна Ахматова">
   <title>Лилии</title>
   <divs>
      <lg type="stanza">
         <l>
            <w>
               <orth>Я</orth>
               <str>Я</str>
            </w>
            <w>
               <orth>лилий</orth>
               <str>л<stress>и</stress>лий</str>
            </w>
            <w>
               <orth>нарвала</orth>
               <!--<str>нарв<stress>а</stress>ла</str>-->
               <str>нарвал<stress>а</stress>
               </str>
            </w>
            <w>
               <orth>прекрасных</orth>
               <str>прекр<stress>а</stress>сных</str>
            </w>
            <w>
               <orth>и</orth>
               <str>и</str>
            </w>
            <w>
               <orth>душистых,</orth>
               <str>душ<stress>и</stress>стых</str>
            </w>
         </l>
         <l>
            <w>
               <orth>Стыдливо-</orth>
               <str>Ст<stress>ы</stress>дливо</str>
            </w>
            <w>
               <orth>замкнутых,</orth>
               <str>з<stress>а</stress>мкнутых</str>
            </w>
            
            <w>
               <orth>как</orth>
               <str>как</str>
            </w>
            <w>
               <orth>дев</orth>
               <str>д<stress>е</stress>в</str>
            </w>
            <w>
               <orth>невинных</orth>
               <str>нев<stress>и</stress>нных</str>
            </w>
            <w>
               <orth>рой,</orth>
               <str>р<stress>о</stress>й</str>
            </w>
         </l>
         <l>
            <w>
               <orth>С</orth>
               <str>С</str>
            </w>
            <w>
               <orth>их</orth>
               <str>
                  их</str>
            </w>
            <w>
               <orth>лепестков,</orth>
               <str>лепестк<stress>о</stress>в</str>
            </w>
            <w>
               <orth>дрожащих</orth>
               <str>дрож<stress>а</stress>щих</str>
            </w>
            <w>
               <orth>и</orth>
               <str>и</str>
            </w>
            <w>
               <orth>росистых,</orth>
               <str>рос<stress>и</stress>стых</str>
            </w>
         </l>
         <l>
            <w>
               <orth>Пила</orth>
               <str>пил<stress>а</stress>
               </str>
            </w>
            <w>
               <orth>я</orth>
               <str>я</str>
            </w>
            <w>
               <orth>аромат</orth>
               <str>аром<stress>а</stress>т</str>
            </w>
            <w>
               <orth>и</orth>
               <str>и</str>
            </w>
            <w>
               <orth>счастье</orth>
               <str>сч<stress>а</stress>стье</str>
            </w>
            <w>
               <orth>и</orth>
               <str>и</str>
            </w>
            <w>
               <orth>покой.</orth>
               <str>пок<stress>о</stress>й</str>
            </w>
         </l>
      </lg>
      <lg type="stanza">
         <l>
            <w>
               <orth>И</orth>
               <str>и</str>
            </w>
            <w>
               <orth>сердце</orth>
               <str>с<stress>е</stress>рдце</str>
            </w>
            <w>
               <orth>трепетно</orth>
               <str>тр<stress>е</stress>петно</str>
            </w>
            <w>
               <orth>сжималось,</orth>
               <str>сжим<stress>а</stress>лось</str>
            </w>
            <w>
               <orth>как</orth>
               <str>как</str>
            </w>
            <w>
               <orth>от</orth>
               <str>от</str>
            </w>
            <w>
               <orth>боли,</orth>
               <str>б<stress>о</stress>ли</str>
               
            </w>
         </l>
         <l>
            <w>
               <orth>А</orth>
               <str>А</str>
            </w>
            <w>
               <orth>бледные</orth>
               <str>бл<stress>е</stress>дные</str>
            </w>
            <w>
               <orth>цветы</orth>
               <str>цвет<stress>ы</stress>
               </str>
            </w>
            <w>
               <orth>качали</orth>
               <str>кач<stress>а</stress>ли</str>
            </w>
            <w>
               <orth>головой,</orth>
               <str>голов<stress>о</stress>й</str>
            </w>
         </l>
         <l>
            <w>
               <orth>И</orth>
               <str>И</str>
            </w>
            <w>
               <orth>вновь</orth>
               <str>вновь</str>
            </w>
            <w>
               <orth>мечтала</orth>
               <str>мечт<stress>а</stress>ла</str>
            </w>
            <w>
               <orth>я</orth>
               <str>я</str>
            </w>
            <w>
               <orth>о</orth>
               <str>о</str>
            </w>
            <w>
               <orth>той</orth>
               <str>т<stress>о</stress>й</str>
            </w>
            <w>
               <orth>далекой</orth>
               <str>дал<stress>ё</stress>кой</str>
            </w>
            <w>
               <orth>воле,</orth>
               <str>в<stress>о</stress>ле</str>
            </w>
         </l>
         <l>
            <w>
               <orth>О</orth>
               <str>О</str>
            </w>
            <w>
               <orth>той</orth>
               <str>т<stress>о</stress>й</str>
            </w>
            <w>
               <orth>стране,</orth>
               <str>стран<stress>е</stress>
               </str>
            </w>
            <w>
               <orth>где</orth>
               <str>где</str>
            </w>
            <w>
               <orth>я</orth>
               <str>я</str>
            </w>
            <w>
               <orth>была</orth>
               <str>был<stress>а</stress>
               </str>
            </w>
            <w>
               <orth>с</orth>
               <str>с</str>
            </w>
            <w>
               <orth>тобой...</orth>
               <str>тоб<stress>о</stress>й</str>
            </w>
         </l>
      </lg>
   </divs>
   <date>22 июня 1904 Одесса</date>
</poem>