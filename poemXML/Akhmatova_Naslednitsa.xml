<?xml version="1.0" encoding="UTF-8"?>
<poem title="Наследница" author="Анна Андреевна Ахматова" date="1953">
  <divs>
    <lg type="epigraph">
      <l>
        <w>
          <orth>От</orth>
          <str>от</str>
        </w>
        <w>
          <orth>царскосельских</orth>
          <str>царскос<stress>е</stress>льских</str>
        </w>
        <w>
          <orth>лип...</orth>
          <str>л<stress>и</stress>п</str>
        </w>
      </l>
      <l>
        <w>
          <orth>Пушкин</orth>
          <str>п<stress>у</stress>шкин</str>
        </w>
      </l>
    </lg>
    <lg type="stanza">
      <l>
        <w>
          <orth>Казалось</orth>
          <str>каз<stress>а</stress>лось</str>
        </w>
        <w>
          <orth>мне,</orth>
          <str>мне</str>
        </w>
        <w>
          <orth>что</orth>
          <str>что</str>
        </w>
        <w>
          <orth>песня</orth>
          <str>п<stress>е</stress>сня</str>
        </w>
        <w>
          <orth>спета</orth>
          <str>сп<stress>е</stress>та</str>
        </w>
      </l>
      <l>
        <w>
          <orth>Средь</orth>
          <str>средь</str>
        </w>
        <w>
          <orth>этих</orth>
          <str><stress>э</stress>тих</str>
        </w>
        <w>
          <orth>опустелых</orth>
          <str>опуст<stress>е</stress>лых</str>
        </w>
        <w>
          <orth>зал.</orth>
          <str>з<stress>а</stress>л</str>
        </w>
      </l>
      <l>
        <w>
          <orth>О,</orth>
          <str>о</str>
        </w>
        <w>
          <orth>кто</orth>
          <str>кт<stress>о</stress></str>
        </w>
        <w>
          <orth>бы</orth>
          <str>бы</str>
        </w>
        <w>
          <orth>мне</orth>
          <str>мн<stress>е</stress></str>
        </w>
        <w>
          <orth>тогда</orth>
          <str>тогд<stress>а</stress></str>
        </w>
        <w>
          <orth>сказал,</orth>
          <str>сказ<stress>а</stress>л</str>
        </w>
      </l>
      <l>
        <w>
          <orth>Что</orth>
          <str>что</str>
        </w>
        <w>
          <orth>я</orth>
          <str>я</str>
        </w>
        <w>
          <orth>наследую</orth>
          <str>насл<stress>е</stress>дую</str>
        </w>
        <w>
          <orth>все</orth>
          <str>все</str>
        </w>
        <w>
          <orth>это:</orth>
          <str><stress>э</stress>то</str>
        </w>
      </l>
      <l>
        <w>
          <orth>Фелицу,</orth>
          <str>фел<stress>и</stress>цу</str>
        </w>
        <w>
          <orth>лебедя,</orth>
          <str>л<stress>е</stress>бедя</str>
        </w>
        <w>
          <orth>мосты</orth>
          <str>мост<stress>ы</stress></str>
        </w>
      </l>
      <l>
        <w>
          <orth>И</orth>
          <str>и</str>
        </w>
        <w>
          <orth>все</orth>
          <str>все</str>
        </w>
        <w>
          <orth>китайские</orth>
          <str>кит<stress>а</stress>йские</str>
        </w>
        <w>
          <orth>затеи,</orth>
          <str>зат<stress>е</stress>и</str>
        </w>
      </l>
      <l>
        <w>
          <orth>Дворца</orth>
          <str>дворц<stress>а</stress></str>
        </w>
        <w>
          <orth>сквозные</orth>
          <str>сквозн<stress>ы</stress>е</str>
        </w>
        <w>
          <orth>галереи</orth>
          <str>галер<stress>е</stress>и</str>
        </w>
      </l>
      <l>
        <w>
          <orth>И</orth>
          <str>и</str>
        </w>
        <w>
          <orth>липы</orth>
          <str>л<stress>и</stress>пы</str>
        </w>
        <w>
          <orth>дивной</orth>
          <str>д<stress>и</stress>вной</str>
        </w>
        <w>
          <orth>красоты.</orth>
          <str>красот<stress>ы</stress></str>
        </w>
      </l>
      <l>
        <w>
          <orth>И</orth>
          <str>и</str>
        </w>
        <w>
          <orth>даже</orth>
          <str>д<stress>а</stress>же</str>
        </w>
        <w>
          <orth>собственную</orth>
          <str>с<stress>о</stress>бственную</str>
        </w>
        <w>
          <orth>тень,</orth>
          <str>т<stress>е</stress>нь</str>
        </w>
      </l>
      <l>
        <w>
          <orth>Всю</orth>
          <str>всю</str>
        </w>
        <w>
          <orth>искаженную</orth>
          <str>искаж<stress>ё</stress>нную</str>
        </w>
        <w>
          <orth>от</orth>
          <str>от</str>
        </w>
        <w>
          <orth>страха,</orth>
          <str>стр<stress>а</stress>ха</str>
        </w>
      </l>
      <l>
        <w>
          <orth>И</orth>
          <str>и</str>
        </w>
        <w>
          <orth>покаянную</orth>
          <str>пока<stress>я</stress>нную</str>
        </w>
        <w>
          <orth>рубаху,</orth>
          <str>руб<stress>а</stress>ху</str>
        </w>
      </l>
      <l>
        <w>
          <orth>И</orth>
          <str>и</str>
        </w>
        <w>
          <orth>замогильную</orth>
          <str>замог<stress>и</stress>льную</str>
        </w>
        <w>
          <orth>сирень.</orth>
          <str>сир<stress>е</stress>нь</str>
        </w>
      </l>
    </lg>
  </divs>
</poem>
