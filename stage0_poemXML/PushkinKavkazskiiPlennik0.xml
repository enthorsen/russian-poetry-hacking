<?xml version="1.0" encoding="UTF-8"?>
<poem author="Александр Сергеевич Пушкин" title="Кавказский пленник" date="1820-21">
    <divs type="chapter">
        <lg>
            <l>
                <w>
                    <orth>В</orth>
                    <str>в</str>
                </w>
                <w>
                    <orth>ауле,</orth>
                    <str>а<stress>у</stress>ле</str>
                </w>
                <w>
                    <orth>на</orth>
                    <str>на</str>
                </w>
                <w>
                    <orth>своих</orth>
                    <str>сво<stress>и</stress>х</str>
                </w>
                <w>
                    <orth>порогах,</orth>
                    <str>пор<stress>о</stress>гах</str>
                </w>
            </l>
            <l>
                <w>
                    <orth>Черкесы</orth>
                    <str>черк<stress>е</stress>сы</str>
                </w>
                <w>
                    <orth>праздные</orth>
                    <str>пр<stress>а</stress>здные</str>
                </w>
                <w>
                    <orth>сидят.</orth>
                    <str>сид<stress>я</stress>т</str>
                </w>
            </l>
            <l>
                <w>
                    <orth>Сыны</orth>
                    <str>(сыны)</str>
                </w>
                <w>
                    <orth>Кавказа</orth>
                    <str>кавк<stress>а</stress>за</str>
                </w>
                <w>
                    <orth>говорят</orth>
                    <str>говор<stress>я</stress>т</str>
                </w>
            </l>
            <l>
                <w>
                    <orth>О</orth>
                    <str>о</str>
                </w>
                <w>
                    <orth>бранных,</orth>
                    <str>бр<stress>а</stress>нных</str>
                </w>
                <w>
                    <orth>гибельных</orth>
                    <str>г<stress>и</stress>бельных</str>
                </w>
                <w>
                    <orth>тревогах,</orth>
                    <str>трев<stress>о</stress>гах</str>
                </w>
            </l>
            <l>
                <w>
                    <orth>О</orth>
                    <str>о</str>
                </w>
                <w>
                    <orth>красоте</orth>
                    <str>красот<stress>е</stress></str>
                </w>
                <w>
                    <orth>своих</orth>
                    <str>сво<stress>и</stress>х</str>
                </w>
                <w>
                    <orth>коней,</orth>
                    <str>кон<stress>е</stress>й</str>
                </w>
            </l>
            <l>
                <w>
                    <orth>О</orth>
                    <str>о</str>
                </w>
                <w>
                    <orth>наслажденьях</orth>
                    <str>наслажд<stress>е</stress>ньях</str>
                </w>
                <w>
                    <orth>дикой</orth>
                    <str>д<stress>и</stress>кой</str>
                </w>
                <w>
                    <orth>неги;</orth>
                    <str>н<stress>е</stress>ги</str>
                </w>
            </l>
            <l>
                <w>
                    <orth>Воспоминают</orth>
                    <str>воспомин<stress>а</stress>ют</str>
                </w>
                <w>
                    <orth>прежних</orth>
                    <str>пр<stress>е</stress>жних</str>
                </w>
                <w>
                    <orth>дней</orth>
                    <str>дн<stress>е</stress>й</str>
                </w>
            </l>
            <l>
                <w>
                    <orth>Неотразимые</orth>
                    <str>неотраз<stress>и</stress>мые</str>
                </w>
                <w>
                    <orth>набеги,</orth>
                    <str>наб<stress>е</stress>ги</str>
                </w>
            </l>
            <l>
                <w>
                    <orth>Обманы</orth>
                    <str>обм<stress>а</stress>ны</str>
                </w>
                <w>
                    <orth>хитрых</orth>
                    <str>х<stress>и</stress>трых</str>
                </w>
                <w>
                    <orth>узденей,</orth>
                    <str>узден<stress>е</stress>й</str>
                </w>
            </l>
            <l>
                <w>
                    <orth>Удары</orth>
                    <str>уд<stress>а</stress>ры</str>
                </w>
                <w>
                    <orth>шашек</orth>
                    <str>ш<stress>а</stress>шек</str>
                </w>
                <w>
                    <orth>их</orth>
                    <str>(их)</str>
                </w>
                <w>
                    <orth>жестоких,</orth>
                    <str>жест<stress>о</stress>ких</str>
                </w>
            </l>
            <l>
                <w>
                    <orth>И</orth>
                    <str>и</str>
                </w>
                <w>
                    <orth>меткость</orth>
                    <str>м<stress>е</stress>ткость</str>
                </w>
                <w>
                    <orth>неизбежных</orth>
                    <str>неизб<stress>е</stress>жных</str>
                </w>
                <w>
                    <orth>стрел,</orth>
                    <str>стр<stress>е</stress>л</str>
                </w>
            </l>
            <l>
                <w>
                    <orth>И</orth>
                    <str>и</str>
                </w>
                <w>
                    <orth>пепел</orth>
                    <str>п<stress>е</stress>пел</str>
                </w>
                <w>
                    <orth>разорённых</orth>
                    <str>(разорённых)</str>
                </w>
                <w>
                    <orth>сел,</orth>
                    <str>с<stress>е</stress>л</str>
                </w>
            </l>
            <l>
                <w>
                    <orth>И</orth>
                    <str>и</str>
                </w>
                <w>
                    <orth>ласки</orth>
                    <str>л<stress>а</stress>ски</str>
                </w>
                <w>
                    <orth>пленниц</orth>
                    <str>пл<stress>е</stress>нниц</str>
                </w>
                <w>
                    <orth>чернооких</orth>
                    <str>черно<stress>о</stress>ких</str>
                </w>
            </l>
        </lg>
    </divs>
</poem>
